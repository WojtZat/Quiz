<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.DialogPane?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.RowConstraints?>

<DialogPane xmlns="http://javafx.com/javafx/8.0.121" xmlns:fx="http://javafx.com/fxml/1" fx:controller="QuizController.ListFrameController">
   <content>
      <GridPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="300.0">
          <columnConstraints>
              <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          </columnConstraints>
          <rowConstraints>
              <RowConstraints maxHeight="356.0" minHeight="10.0" prefHeight="353.0" vgrow="SOMETIMES" />
              <RowConstraints maxHeight="195.0" minHeight="10.0" prefHeight="37.0" vgrow="SOMETIMES" />
          </rowConstraints>
          <children>
              <HBox alignment="CENTER" prefHeight="27.0" prefWidth="292.0" GridPane.hgrow="ALWAYS" GridPane.rowIndex="1" GridPane.vgrow="ALWAYS">
                  <children>
                      <Button fx:id="ListFrameDeleteButton" mnemonicParsing="false" onAction="#listFrameDeleteQuestion" text="Delete question" HBox.hgrow="ALWAYS" />
                      <Button fx:id="ListFrameClearListButton" mnemonicParsing="false" text="Clear list" HBox.hgrow="ALWAYS" onAction="#clearListListFrame" >
                          <HBox.margin>
                              <Insets left="100.0" />
                          </HBox.margin>
                      </Button>
                  </children>
                  <GridPane.margin>
                      <Insets />
                  </GridPane.margin>
                  <padding>
                      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
                  </padding>
              </HBox>
              <ListView fx:id="listFrameView" prefHeight="200.0" prefWidth="200.0">
              </ListView>
          </children>
          <padding>
              <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
          </padding>
      </GridPane>
   </content>
</DialogPane>
